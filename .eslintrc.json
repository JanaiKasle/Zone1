{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": { 
        "no-undef": "off",
        "no-unused-vars": "warn",
        "quotes": [2, "double", { "avoidEscape": true }],
        "semi": ["warn", "always"],
        "no-multiple-empty-lines": "off"
    }
}
